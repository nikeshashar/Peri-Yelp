<% if @restaurants.any? %>
  <% @restaurants.each do |restaurant| %>
    <h2><%= restaurant.name %></h2>
    <h2><%= restaurant.cuisine %></h2>
    <h2><%= restaurant.phone_number %></h2>
  <% end %>
<% else %>
  No restaurants yet
<% end %>

<%= link_to 'Add a restaurant', new_restaurant_path %>
